package cn.itcast.p.bean;


public class Person {
	private String name;
	private int age;
	
	@Override
	public int hashCode() {
		return name.hashCode()+age;
		
	} 
	@Override
	public boolean equals(Person arg0) {
		return this.age==arg0.age&&this.name==arg0.name;
		
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
}

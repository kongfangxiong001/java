package cn.itcast.p2.wrapper.demo;

public class WrapperDemo {

	public static void main(String[] args) {
		/*
		 * 基本数据类型包装类
		 * 8个包装类
		 * byte			Byte
		 * short		Short
		 * int			Integer
		 * long			Long
		 * double		Double
		 * char			Character
		 * boolean		Boolean
		 */
		
		int num;
		System.out.println(Integer.MIN_VALUE);
		System.out.println(Integer.toBinaryString(-6));
		
		int num = 4;
		Integer i = new Integer(num);
		
	}

}

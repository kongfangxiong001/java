package cn.itcast.p1.string.demo;

public class StringMethodDemo {

	public static void main(String[] args) {
		/*
		 * 获取长度
		 *  int length();
		 * 根据位置获取字符
		 * 	char charAt(int index);
		 * 获取第一次/最后一次出现的位置
		 *  int indexOf();
		 */
		StringMethodDemo_1();
	}

	private static void StringMethodDemo_1() {
		String s = "abcdef";
		System.out.println("index:"+s.indexOf("k"));//判断位置或存在
	}
}
